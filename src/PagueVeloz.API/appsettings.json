{
  "ConnectionStrings": {
    "DefaultConnection": "",
    "ReadConnection": ""
  },
  "EnableDatabasePurge": false,
  "Resilience": {
    "Default": {
      "TimeoutSeconds": 0,
      "MaxRetryAttempts": 0,
      "RetryDelayMs": 0,
      "CircuitBreakerFailureRatio": 0,
      "CircuitBreakerSamplingDurationSeconds": 0,
      "CircuitBreakerMinimumThroughput": 0,
      "CircuitBreakerBreakDurationSeconds": 0
    },
    "Database": {
      "TimeoutSeconds": 0,
      "MaxRetryAttempts": 0,
      "RetryDelayMs": 0,
      "CircuitBreakerFailureRatio": 0,
      "CircuitBreakerSamplingDurationSeconds": 0,
      "CircuitBreakerMinimumThroughput": 0,
      "CircuitBreakerBreakDurationSeconds": 0
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore": "Warning",
        "PagueVeloz.Infrastructure.Caching": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
